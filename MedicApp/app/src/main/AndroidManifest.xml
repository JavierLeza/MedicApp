<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.medic.medicapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/heartbeat"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/heartbeat"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".PatientActivity" />
        <activity android:name=".LogInActivity" />
        <activity android:name=".RegisterActivity" />
        <activity android:name=".AddPatientActivity" />
        <activity
            android:name=".PatientSymptomsActivity"
            android:label="@string/title_activity_patient_symptoms"
            android:theme="@style/AppTheme.NoActionBar"></activity>
    </application>

</manifest>